{
  "name": "n8n-nodes-pinterest",
  "version": "0.1.0",
  "description": "Comprehensive Pinterest node for n8n workflow automation platform with full API v5 support including pins, boards, media upload, analytics, and search functionality",
  "keywords": [
    "n8n-community-node-package",
    "pinterest",
    "social-media",
    "automation",
    "workflow",
    "api",
    "pins",
    "boards",
    "media-upload",
    "analytics",
    "oauth2"
  ],
  "license": "MIT",
  "homepage": "https://github.com/OfficialMoAdel/n8n-nodes-pinterest#readme",
  "author": {
    "name": "Mohamed Adel",
    "email": "Mohamed.adel.3972@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficialMoAdel/n8n-nodes-pinterest.git"
  },
  "bugs": {
    "url": "https://github.com/OfficialMoAdel/n8n-nodes-pinterest/issues"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run compile && npm run copy-assets",
    "clean": "rimraf dist",
    "compile": "tsc -p tsconfig.build.json",
    "copy-assets": "gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "format:check": "prettier nodes credentials --check",
    "lint": "eslint nodes credentials package.json",
    "lint:fix": "eslint nodes credentials package.json --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:integration:watch": "jest --config jest.integration.config.js --watch",
    "test:integration:coverage": "jest --config jest.integration.config.js --coverage",
    "test:all": "npm run test && npm run test:integration",
    "test:build": "npm run build && npm run test:package",
    "test:package": "node scripts/test-package.js",
    "test:deployment": "node scripts/test-deployment.js",
    "validate": "npm run type-check && npm run lint && npm run format:check",
    "validate:build": "node scripts/validate-build.js",
    "validate:build-only": "node scripts/validate-build-only.js",
    "prepack": "npm run validate && npm run build && npm run test:deployment",
    "prepublishOnly": "npm run test:all && eslint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist/nodes/Pinterest/",
    "dist/credentials/PinterestOAuth2Api.credentials.js",
    "dist/credentials/PinterestOAuth2Api.credentials.d.ts",
    "README.md",
    "LICENSE.md",
    "CHANGELOG.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/PinterestOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Pinterest/Pinterest.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "del": "^7.1.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "peerDependenciesMeta": {
    "n8n-workflow": {
      "optional": false
    }
  }
}